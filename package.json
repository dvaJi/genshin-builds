{
  "name": "genshin-builds",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "lint": "next lint",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "generate-all": "npm run generate-sitemap && npm run generate-common-dict",
    "generate-sitemap": "node ./scripts/sitemap.mjs",
    "generate-common-dict": "node ./scripts/generate-common-dict.js",
    "analize": "cross-env ANALYZE=true npm run build",
    "start": "next start",
    "type-check": "tsc",
    "db:generate": "prisma generate --schema=./db/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=./db/schema.prisma --skip-seed",
    "db:reset": "prisma migrate reset --schema=./db/schema.prisma --skip-seed",
    "db:push": "prisma db push --schema=./db/schema.prisma",
    "db:seed": "npx prisma db seed",
    "db:studio": "prisma studio --schema=./db/schema.prisma",
    "now-build": "pnpm db:generate --schema=./db/schema.prisma && next build"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.430.0",
    "@aws-sdk/s3-request-presigner": "^3.430.0",
    "@freestar/pubfig-adslot-react-component": "^3.6.0",
    "@lexical/react": "^0.12.2",
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/mdx": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@nanostores/persistent": "^0.9.1",
    "@nanostores/react": "^0.7.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@next/mdx": "^13.5.5",
    "@planetscale/database": "^1.11.0",
    "@prisma/adapter-planetscale": "^5.4.3",
    "@prisma/client": "^5.4.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@tanstack/react-table": "^8.10.7",
    "@vercel/og": "^0.5.19",
    "axios": "^1.5.1",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "framer-motion": "^10.16.4",
    "genshin-data": "^0.35.0",
    "gray-matter": "^4.0.3",
    "hsr-data": "^0.4.0",
    "leaflet": "^1.9.4",
    "lexical": "^0.12.2",
    "nanostores": "^0.9.4",
    "next": "^13.5.5",
    "next-auth": "4.23.0",
    "next-mdx-remote": "^4.4.1",
    "next-pwa": "^5.6.0",
    "next-sitemap": "^4.2.3",
    "next-superjson-plugin": "^0.5.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.47.0",
    "react-icons": "^4.11.0",
    "react-lazy-load-image-component": "^1.6.0",
    "react-leaflet": "^4.2.1",
    "react-tooltip": "^5.21.5",
    "react-wrap-balancer": "^1.1.0",
    "remark-gfm": "3.0.1",
    "slugify": "^1.6.6",
    "superjson": "^1.13.3",
    "swr": "^2.2.4",
    "tof-builds": "0.10.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.8.1",
    "@cloudflare/next-on-pages": "^1.7.1",
    "@next/bundle-analyzer": "^13.5.5",
    "@next/env": "^13.5.5",
    "@types/leaflet": "^1.9.7",
    "@types/node": "^18.18.6",
    "@types/react": "^18.2.29",
    "@types/react-dom": "^18.2.13",
    "@types/react-lazy-load-image-component": "^1.6.2",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "eslint": "8.47.0",
    "eslint-config-next": "^13.5.5",
    "eslint-plugin-next-on-pages": "^1.7.1",
    "globby": "^13.2.2",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "prisma": "^5.4.2",
    "tailwindcss": "^3.3.3",
    "typescript": "5.1.6",
    "vercel": "^32.4.1"
  }
}
