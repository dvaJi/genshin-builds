{
  "name": "genshin-builds",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "lint": "next lint",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "generate-all": "npm run generate-sitemap && npm run generate-common-dict",
    "generate-sitemap": "node ./scripts/sitemap.mjs",
    "generate-common-dict": "node ./scripts/generate-common-dict.js",
    "analize": "cross-env ANALYZE=true npm run build",
    "start": "next start",
    "type-check": "tsc",
    "db:generate": "prisma generate --schema=./db/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=./db/schema.prisma --skip-seed",
    "db:reset": "prisma migrate reset --schema=./db/schema.prisma --skip-seed",
    "db:push": "prisma db push --schema=./db/schema.prisma",
    "db:seed": "npx prisma db seed",
    "db:studio": "prisma studio --schema=./db/schema.prisma",
    "now-build": "pnpm db:generate --schema=./db/schema.prisma && next build"
  },
  "dependencies": {
    "@freestar/pubfig-adslot-react-component": "^3.6.0",
    "@nanostores/persistent": "^0.9.0",
    "@nanostores/react": "^0.7.1",
    "@prisma/client": "^4.15.0",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@tanstack/react-table": "^8.9.2",
    "clsx": "^1.2.1",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.0",
    "framer-motion": "^10.12.16",
    "genshin-data": "^0.31.2",
    "gray-matter": "^4.0.3",
    "hsr-data": "^0.1.0",
    "leaflet": "^1.9.4",
    "nanostores": "^0.9.2",
    "next": "^13.4.6",
    "next-pwa": "^5.6.0",
    "next-sitemap": "^4.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.9.0",
    "react-lazy-load-image-component": "^1.6.0",
    "react-leaflet": "^4.2.1",
    "remark": "^14.0.3",
    "remark-html": "^15.0.2",
    "swr": "^2.1.5",
    "tof-builds": "^0.8.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.8.0",
    "@next/bundle-analyzer": "^13.4.6",
    "@next/env": "^13.4.6",
    "@types/leaflet": "^1.9.3",
    "@types/node": "^18.16.18",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@types/react-lazy-load-image-component": "^1.5.3",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-config-next": "^13.4.6",
    "globby": "^13.1.4",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "prisma": "^4.15.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3"
  }
}
